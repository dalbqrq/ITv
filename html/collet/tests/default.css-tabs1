/* GLOBAL HTML STYLES */
body {
	margin: 0;
	padding: 0;
	font-family: Georgia, "Times New Roman", Times, serif;
	font-size: 13px;
}

a		{ color: #135293; }
a:hover		{ text-decoration: none; }
blockquote	{ font-style: italic; }


/* CONTAINER */
#container {
	width: 980px;
	margin: 0 auto;
	background: #E5E5E5;
	background: #F8F8F8;
	color: #666666;
}


/* HEADER */
#header {
	height: 60px;
	background: #3333FF;
	background: #000000;
}

/* MAINNAV */
#mainnav {
	height: 100%;
	padding: 00px 40px 0px 0px;
	text-size: 30px;
	text-align: right;
	color: #FFFFFF;
	background: #888888;
